pipeline {
  
    agent any
  
    stages {
  
    	stage("build") {
        
        	steps {
          		echo "building the app..."
			}			
		}

		stage("test") {
        
        	steps {
        		"testing the app..."  
			}			
		}

		stage("deploy") {
        
        	steps {
          		"deploying the app..."
			}			
		}
	}
}
